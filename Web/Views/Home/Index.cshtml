@model List<string>
@{
    ViewData["Title"] = "Home";
}

<h1>Home</h1>
<h2>Laptop List</h2>
<table>
    <tr>
        @for (int i = 0; i < Model.Count; i++)
        {
            <th>
                <img src="@Model[i]" class="img" alt="Image @(i+1)" />
            </th>
        }
    </tr>
</table>

